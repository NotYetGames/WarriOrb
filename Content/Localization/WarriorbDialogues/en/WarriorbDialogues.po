#. Key:	AC0DE5F1402FBAAFE6587B827D463C3C
#. SourceLocation:	/Game/SO/Dialogues/Wizard/Chapter1/TL_SoulkeeperHint.TL_SoulkeeperHint:DlgNode_Speech_1.Text
#: /Game/SO/Dialogues/Wizard/Chapter1/TL_SoulkeeperHint.TL_SoulkeeperHint:DlgNode_Speech_1.Text
msgctxt "Dialogue_TL_SoulkeeperHint"
msgid "Use the Soulstone to summon the Keeper."
msgstr "Use the Soulstone to summon the Keeper."

#. Key:	031849B3493F1528A0146DA55BD7F00F
#. SourceLocation:	/Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_3.Text
#: /Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_3.Text
msgctxt "Dialogue_TL_TakeSoulkeeperHint"
msgid "It would be easier if I could just simply summon her back to me..."
msgstr "It would be easier if I could just simply summon her back to me..."

#. Key:	259EE9E14B86B2597D5F4EA60831D269
#. SourceLocation:	/Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_4.Text
#: /Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_4.Text
msgctxt "Dialogue_TL_TakeSoulkeeperHint"
msgid "Be more careful the next time..."
msgstr "Be more careful the next time..."

#. Key:	86266BC54CC02BE988EA24B18BF76359
#. SourceLocation:	/Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_1.Text
#: /Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_1.Text
msgctxt "Dialogue_TL_TakeSoulkeeperHint"
msgid "Now go back to the Soulkeeper."
msgstr "Now go back to the Soulkeeper."

#. Key:	DCA8CF9A4CFF4644FBD42EADBFC6E773
#. SourceLocation:	/Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_2.Text
#: /Game/SO/Dialogues/Wizard/Chapter1/TL_TakeSoulkeeperHint.TL_TakeSoulkeeperHint:DlgNode_Speech_2.Text
msgctxt "Dialogue_TL_TakeSoulkeeperHint"
msgid "Er... It's still with me."
msgstr "Er... It's still with me."

#. Key:	012DDB8E4423EB98285155B3E1EFBF9A
